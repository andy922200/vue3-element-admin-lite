{
  "name": "vue3-element-admin-lite",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "repository": "https://github.com/andy922200/vue3-element-admin-lite.git",
  "author": "Andy Lien fork from '有来开源组织, v2.4.0'",
  "license": "MIT",
  "scripts": {
    "dev": "vite serve",
    "build": "vite build",
    "serve": "vue-tsc --noEmit && vite build && vite preview --port 3000",
    "preview": "vite preview --port 3000",
    "prepare": "husky install",
    "lint": "prettier --write . && eslint \"./**/*.{ts,js,d.ts,vue}\" --fix && stylelint  \"**/*.{css,scss,vue}\" --fix",
    "prettier": "prettier --write .",
    "eslint": "eslint \"./**/*.{ts,js,d.ts,vue}\" --fix",
    "stylelint": "stylelint  \"**/*.{css,scss,vue}\" --fix",
    "test": "jest"
  },
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.15.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write .",
      "eslint \"./**/*.{ts,js,d.ts,vue}\" --fix"
    ],
    "*.{cjs,json}": [
      "prettier --write ."
    ],
    "*.{vue}": [
      "prettier --write .",
      "eslint \"./**/*.{ts,js,d.ts,vue}\" --fix",
      "stylelint  \"**/*.{css,scss,vue}\" --fix"
    ],
    "*.{scss,css}": [
      "stylelint  \"**/*.{css,scss,vue}\" --fix",
      "prettier --write ."
    ],
    "*.md": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.1.2",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "5.1.10",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.2.2",
    "element-plus": "^2.3.6",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.2",
    "screenfull": "^6.0.0",
    "vue": "^3.3.1",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify-json/ep": "^1.1.10",
    "@types/codemirror": "^5.60.7",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.3.3",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vue/test-utils": "^2.4.0",
    "@vue/vue3-jest": "^29.2.4",
    "autoprefixer": "^10.4.14",
    "axios-mock-adapter": "^1.21.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "fast-glob": "^3.2.11",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.5.0",
    "jest-html-reporter": "^3.10.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.58.3",
    "stylelint": "^15.5.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "11.0.0 ",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.6.5 "
  }
}
